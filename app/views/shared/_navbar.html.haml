- content_for :styles do
  = stylesheet_link_tag 'shared_navbar'

/ Navbar. In the future we can include instructor navbars here too
- if @authenticated_user
  - if @authenticated_user.is_admin?
    = render "admins/navbar"
  - elsif @authenticated_user.is_reader?
    = render "reader/reader_navbar"
  - else
    = render "students/loggedin_student_navbar"
- else
  = render "shared/general_navbar"